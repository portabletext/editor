{
  "extends": "//",
  "linter": {
    "rules": {
      "complexity": {
        "useLiteralKeys": "off"
      },
      "style": {
        "noRestrictedImports": {
          "level": "error",
          "options": {
            "paths": {
              "@sanity/types": "Import from 'types/sanity-types' instead to maintain visibility over @sanity/types usage."
            }
          }
        }
      }
    }
  }
}
