    "vite": "^5.4.4"
  },
  "peerDependencies": {
    "@sanity/block-tools": "^3.57.2",
    "@sanity/schema": "^3.57.2",
    "@sanity/types": "^3.57.2",
    "@sanity/util": "^3.57.2",
    "react": "^16.9 || ^17 || ^18",
    "rxjs": "^7.8.1",
    "styled-components": "^6.1.13"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
