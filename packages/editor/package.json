{
  "name": "@portabletext/editor",
  "version": "1.4.0",
  "description": "Portable Text Editor made in React",
  "keywords": [
    "sanity",
    "realtime",
    "content",
    "portable-text-editor",
    "structure",
    "collaborative",
    "editor",
    "text",
    "portable-text"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/portabletext/editor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/portabletext/editor.git",
    "directory": "packages/editor"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --check --clean",
    "check:lint": "biome lint .",
    "check:types": "tsc",
    "check:react-compiler": "eslint --cache --no-inline-config --no-eslintrc --ext .cjs,.mjs,.js,.jsx,.ts,.tsx --parser @typescript-eslint/parser --plugin react-compiler --plugin react-hooks --rule 'react-compiler/react-compiler: [warn]' --rule 'react-hooks/rules-of-hooks: [error]' --rule 'react-hooks/exhaustive-deps: [error]' src",
    "clean": "del .turbo && del lib && del node_modules",
    "dev": "pkg-utils watch",
    "lint:fix": "biome lint --write .",
    "prepublishOnly": "turbo run build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:e2e-legacy": "jest --config=e2e-tests/e2e.config.ts",
    "test:e2e-legacy:watch": "jest --config=e2e-tests/e2e.config.ts --watch"
  },
  "dependencies": {
    "@portabletext/patches": "1.1.0",
    "@xstate/react": "^4.1.3",
    "debug": "^4.3.4",
    "is-hotkey-esm": "^1.0.0",
    "lodash": "^4.17.21",
    "react-compiler-runtime": "19.0.0-beta-6fc168f-20241025",
    "slate": "0.110.2",
    "slate-dom": "^0.111.0",
    "slate-react": "0.111.0",
    "use-effect-event": "^1.0.2",
    "xstate": "^5.18.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@playwright/test": "1.48.2",
    "@portabletext/toolkit": "^2.0.15",
    "@sanity/block-tools": "^3.62.3",
    "@sanity/diff-match-patch": "^3.1.1",
    "@sanity/gherkin-driver": "workspace:^",
    "@sanity/pkg-utils": "^6.11.8",
    "@sanity/schema": "^3.62.3",
    "@sanity/types": "^3.62.3",
    "@sanity/ui": "^2.8.18",
    "@sanity/util": "^3.62.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.5",
    "@types/lodash": "^4.17.13",
    "@types/node": "^18.19.8",
    "@types/node-ipc": "^9.2.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "~8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/browser": "^2.1.4",
    "babel-plugin-react-compiler": "beta",
    "dotenv": "^16.4.5",
    "eslint": "8.57.1",
    "eslint-plugin-react-compiler": "beta",
    "eslint-plugin-react-hooks": "^5.0.0",
    "express": "^4.21.1",
    "express-ws": "^5.0.2",
    "jest": "^29.7.0",
    "jest-dev-server": "^10.1.4",
    "jest-environment-node": "^29.7.0",
    "jsdom": "^25.0.1",
    "node-ipc": "npm:@node-ipc/compat@9.2.5",
    "playwright": "^1.48.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1",
    "styled-components": "^6.1.13",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "vitest-browser-react": "^0.0.3"
  },
  "peerDependencies": {
    "@sanity/block-tools": "^3.62.3",
    "@sanity/schema": "^3.62.3",
    "@sanity/types": "^3.62.3",
    "@sanity/util": "^3.62.3",
    "react": "^16.9 || ^17 || ^18",
    "rxjs": "^7.8.1",
    "styled-components": "^6.1.13"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
